{% extends 'base.html.twig' %}

{% block body %}
<h1 class='text-center'>Users</h1>
{% if article %}
    <table class="table table-striped table-inverse table-responsive">
        <thead class="thead-inverse">
            <tr>
                <th>id</th>
                <th>Room</th>
                <th>Program</th>
                <th>Date</th>
                <th>Actions</th>
            </tr>
            </thead>
            <tbody>
            {% for item in article %}
                <tr>
                    <td scope="row">{{item}}</td>
                    <td>{{item}}</td>
                    <td></td>
                    <td></td>
                    <td><a href="" class="btn btn-dark">Show</a></td>
                </tr>
            {% endfor %}
            </tbody>
    </table>

{% else %}
    <p> No reservation </p>

{% endif %}

{% endblock %}